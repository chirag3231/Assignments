package program1;
public class pattern3 
{
    public static void main(String[] args)
    {
        
        for(int i=1;i<=6;i++)
        {
            for(int j=1;j<i;j++)
            {
                System.out.print(j);
                System.out.print(" ");
                
            } 
            System.out.println("");
        }
    }
}

